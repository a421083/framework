{
	"name": "shadon/framework",
	"description": "shadon php framework",
	"keywords": [
		"api",
		"phalcon",
		"modules"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Shadon",
			"homepage": "https://www.shadon.org"
		}
	],
	"require": {
		"php": "^7.1.3",
		"aliyuncs/oss-sdk-php": "^2.2",
		"erusev/parsedown": "^1.6",
		"guzzlehttp/guzzle": "^6.2.3",
		"mongodb/mongodb": "^1.1.2",
		"monolog/monolog": "^1.22.1",
		"php-amqplib/thumper": "^0.5.0",
		"phpdocumentor/reflection-docblock": "^3.0.2",
		"predis/predis": "^1.1.1",
		"ruflin/elastica": "^5.3",
		"swiftmailer/swiftmailer": "^6.0.1",
		"tightenco/collect": "^5.4",
		"vlucas/phpdotenv": "^2.4"
	},
	"require-dev": {
		"techpivot/phalcon-ci-installer": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Shadon\\": "src"
		},
		"files": [
			"Resources/functions/functions_include.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"config": {
		"sort-packages": true
	}
}
